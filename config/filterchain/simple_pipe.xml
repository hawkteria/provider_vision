<?xml version="1.0"?>
<Filterchain>
	<Filter name="Rotate">
		<enable value="1" />
		<Rotation_type value="3" />
	</Filter>
	<Filter name="Rotate">
		<enable value="0" />
		<Rotation_type value="3" />
	</Filter>
	<Filter name="BackgroundSubstract">
		<Enable value="1" />
		<Show_blurred value="0" />
		<Blur_size value="255" />
		<Sigma value="10" />
	</Filter>
	<Filter name="SubtractAllPlanes">
		<enable value="1" />
		<Plane_1 value="4" />
		<Plane_2 value="3" />
		<Plane_3 value="0" />
		<Invert_plane_1 value="1" />
		<Invert_plane_2 value="1" />
		<Invert_plane_3 value="0" />
		<Weight_Plane_1 value="1.000000" />
		<Weight_Plane_2 value="1.000000" />
		<Weight_Plane_3 value="0.000000" />
	</Filter>
	<Filter name="Blurr">
		<Enable value="1" />
		<Type value="1" />
		<Kernel_size value="2" />
	</Filter>
	<Filter name="StatsThreshold">
		<Enable value="1" />
		<Min_thresh value="20" />
		<Mean_multiplier value="1.000000" />
		<Standard_deviation_multiplier value="1.000000" />
	</Filter>
	<Filter name="Threshold">
		<Enable value="0" />
		<Threshold_type value="5" />
		<Max_value value="20" />
	</Filter>
	<Filter name="Morphology">
		<Enable value="1" />
		<Morphology_type value="4" />
		<Kernel_type value="0" />
		<Iteration value="1" />
		<Kernel_size value="5" />
	</Filter>
	<Filter name="Erode">
		<Enable value="0" />
		<Square_kernel value="1" />
		<Kernel_type value="1" />
		<Width value="1" />
		<Height value="1" />
		<Iteration value="1" />
	</Filter>
	<Filter name="Dilate">
		<Enable value="0" />
		<Square_kernel value="1" />
		<Kernel_type value="0" />
		<Width value="1" />
		<Height value="1" />
		<Iteration value="1" />
	</Filter>
	<Filter name="PipeDetector">
		<Enable value="1" />
		<Debug_contour value="1" />
		<Look_for_Rectangle value="0" />
		<Min_area value="100.000000" />
	</Filter>
</Filterchain>
